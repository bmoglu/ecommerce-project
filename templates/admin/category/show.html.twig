{% extends 'adminbase.html.twig' %}

{% block title %}Category{% endblock %}

{% block body %}
<section role="main" class="content-body">
    <header class="page-header">
        <h2>Show Page</h2>

        <div class="right-wrapper pull-right">
            <ol class="breadcrumbs">
                <li>
                    <a href="index.html">
                        <i class="fa fa-home"></i>
                    </a>
                </li>
                <li><span>Pages</span></li>
                <li><span>Blank Page</span></li>
            </ol>

            <a class="sidebar-right-toggle" data-open="sidebar-right"><i class="fa fa-chevron-left"></i></a>
        </div>
    </header>


    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ category.id }}</td>
        </tr>
        <tr>
            <th>Parentid</th>
            <td>{{ category.parentid }}</td>
        </tr>
        <tr>
            <th>Title</th>
            <td>{{ category.title }}</td>
        </tr>
        <tr>
            <th>Keywords</th>
            <td>{{ category.keywords }}</td>
        </tr>
        <tr>
            <th>Image</th>
            <td>{{ category.image }}</td>
        </tr>
        <tr>
            <th>Status</th>
            <td>{{ category.status }}</td>
        </tr>
        <tr>
            <th>Created_at</th>
            <td>{{ category.createdAt ? category.createdAt|date('Y-m-d H:i:s') : '' }}</td>
        </tr>
        <tr>
            <th>Updated_at</th>
            <td>{{ category.updatedAt ? category.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_category_index') }}">back to list</a>

    <a href="{{ path('admin_category_edit', {'id': category.id}) }}">edit</a>

    {{ include('admin/category/_delete_form.html.twig') }}
    <!-- start: page -->
    <!-- end: page -->
</section>

{% endblock %}